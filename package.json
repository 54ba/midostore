{
  "name": "next_js",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:simple": "next build",
    "build:standalone": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/db-seed.ts",
    "db:reset": "prisma migrate reset --force",
    "seed:reviews": "tsx scripts/seed-reviews.ts",
    "enhanced:reviews": "tsx scripts/enhanced-review-seeder.ts",
    "scrape:products": "tsx scripts/scrape-products.ts",
    "ai:setup": "./scripts/setup-ai-analytics.sh",
    "ai:start": "cd ai && ./start_ai_service.sh",
    "ai:train": "cd ai && source venv/bin/activate && python recommendation_engine.py --train",
    "services:start": "./scripts/start-dynamic-services.sh",
    "services:web3": "./scripts/start-web3-crypto-services.sh",
    "services:status": "node dynamic-config.js --save",
    "services:config": "node dynamic-config.js",
    "services:env-template": "node dynamic-config.js --env-template",
    "test:all": "npm run test:env && npm run test:orchestrator && npm run test:agents && npm run test:roles",
    "test:env": "node scripts/test-env-setup.js",
    "test:orchestrator": "node scripts/test-ai-orchestrator.js",
    "test:agents": "node scripts/test-ai-agent-supervisor.js",
    "test:roles": "node scripts/test-role-management.js",
    "test:reviews": "tsx scripts/test-review-system.ts",
    "test:apis": "node scripts/test-all-apis.js",
    "test:auth": "node scripts/test-auth-protection.js",
    "test:scraping": "node scripts/test-scraping-services.js",
    "test:bulk-pricing": "node scripts/test-bulk-pricing.js",
    "test:advertising": "node scripts/test-advertising-module.js",
    "test:enhanced": "node scripts/test-enhanced-features.js",
    "test:decentralized": "node scripts/test-decentralized-system.js",
    "test:keyless": "./scripts/test-keyless-mode.sh",
    "env:setup": "./scripts/setup-env-from-netlify.sh",
    "env:update": "./scripts/update-env-from-netlify.sh",
    "env:check": "node scripts/check-env.js",
    "netlify:build": "chmod +x scripts/netlify-build.sh && ./scripts/netlify-build.sh",
    "netlify:build:optimized": "chmod +x scripts/netlify-build.sh && ./scripts/netlify-build.sh && echo 'âœ… Optimized build completed - functions are under 250MB limit'",
    "netlify:clean": "./scripts/clean-for-netlify.sh",
    "netlify:deploy": "./scripts/clean-for-netlify.sh && npm run build && netlify deploy --prod",
    "netlify:deploy:optimized": "./scripts/deploy-netlify-optimized.sh",
    "netlify:deploy:simple": "./scripts/deploy-netlify-simple.sh",
    "netlify:deploy:ultra": "./scripts/deploy-netlify-ultra-lightweight.sh",
    "netlify:deploy:static": "./scripts/deploy-netlify-no-functions.sh",
    "netlify:check": "node scripts/check-function-sizes.js",
    "netlify:setup": "npm run netlify:install && npm run netlify:env:check",
    "netlify:install": "npm ci --production=false",
    "netlify:db:setup": "npm run db:generate && npm run db:push",
    "netlify:db:seed": "npm run db:seed",
    "netlify:ai:setup": "cd ai && python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
    "deploy:direct": "./scripts/deploy-direct.sh",
    "deploy:status": "node scripts/build-status.js",
    "clean": "npm run clean:build && npm run clean:cache",
    "clean:build": "rm -rf .next out .vercel .netlify/cache",
    "clean:cache": "rm -rf node_modules/.cache .next/cache",
    "setup:full": "npm run env:setup && npm run ai:setup && npm run db:reset",
    "start:full": "npm run services:start",
    "start:web3": "npm run services:web3",
    "monitor": "node scripts/build-status.js --watch",
    "health": "node scripts/check-services.js",
    "build:deploy": "./scripts/build-and-deploy.sh",
    "quick:start": "./scripts/quick-start.sh",
    "services:manage": "./scripts/service-manager.sh"
  },
  "dependencies": {
    "@ethersproject/contracts": "^5.8.0",
    "@faker-js/faker": "^9.9.0",
    "@hookform/resolvers": "^5.2.1",
    "@langchain/anthropic": "^0.3.26",
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@prisma/client": "^6.14.0",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "currency.js": "^2.0.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "json2csv": "^6.0.0-alpha.2",
    "langchain": "^0.3.31",
    "lodash": "^4.17.21",
    "lucide-react": "^0.540.0",
    "netlify": "^23.2.1",
    "next": "^15.5.0",
    "next-intl": "^4.3.4",
    "node-cron": "^4.2.1",
    "prisma": "^6.14.0",
    "puppeteer": "^24.17.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "stripe": "^18.4.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.12.1",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^24.3.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.5.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "^5",
    "wait-on": "^8.0.0"
  },
  "build": {
    "appId": "com.taskernet.app",
    "productName": "TaskerNet",
    "asar": true,
    "files": [
      "package.json"
    ],
    "extraResources": [
      {
        "from": ".next/standalone",
        "to": ".next/standalone"
      },
      {
        "from": ".next/static",
        "to": ".next/static"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}